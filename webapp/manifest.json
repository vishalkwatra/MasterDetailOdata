{
	"_version": "1.1.0",
		"sap.app": {
		"_version": "1.3.0",
		"id": "oil.ds.fruits",
		"type": "application",
		"title": "{{appTitle}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources":{
			"myService":{
				"uri": "/sap/opu/odata/sap/ZVK_PRODUCT_SUPPLIER_SRV",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
			
			
			
		},
		"description": "{{appDesc}}",
		"tags": {
			"keywords": [
				"{{kw1}}",
				"{{kw2}}"
			]
		},
		"ach": "IS-OIL-DS",
		"resources": "resources.json"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [],
		"icons": {
			"icon": "sap-icon://accelerated"
		}
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"dependencies": {
			"minUI5Version": "1.44",
			"libs": {
				"sap.m": {
					"minVersion": "1.44"
				},
				"sap.ui.core": {
					"minVersion": "1.44"
				},
				"sap.ui.layout": {
					"minVersion": "1.44"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"resources": {
			"css": [{
				"uri": "css/style.css",
				"id": ""
			}]
		},
		"models": {
			"":{
				"dataSource": "myService"
			},
			"local": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "models/mockData/fruit.json"
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {},
				"uri": "i18n/i18n.properties"
			}
		},
		"rootView": {
			"viewName": "oil.ds.view.App",
			"type": "XML"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "oil.ds.view",
				"viewType": "XML",
				"controlId": "myApp"
			},
			"routes": [{
					"pattern": "",
					"name": "master",
					"target": ["vishal", "kwatra"]
				}, {
					"pattern": "{productId}",
					"name": "productendpoint",
					"target": ["vishal", "rani"]
				}

			],
			"targets": {
				"vishal": {
					"viewName": "View1",
					"controlAggregation": "masterPages",
					"viewID": "idView1",
					"viewLevel": 1
				},
				"kwatra": {
					"viewName": "Empty",
					"controlAggregation": "detailPages",
					"viewID": "idEmpty",
					"viewLevel": 1
				},
				"manshu": {
					"viewName": "View1",
					"controlAggregation": "masterPages",
					"viewID": "idView1",
					"viewLevel": 1
				},
				"rani": {
					"viewName": "View2",
					"controlAggregation": "detailPages",
					"viewID": "idView2",
					"viewLevel": 1
				}

			}
		}
	}

}